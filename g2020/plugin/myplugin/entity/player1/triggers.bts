CASH_BOX_BUTTON_SURE
{
    $money = ToNumber($name)
    If(not IsNumber($money))
    {
        ShowTip(entity = $obj1, tipType = 2, textKey = "gui_cashbox_only_number", time = 40)
        Return()
    }

    If($money // 1 ~= $money)
    {
        ShowTip(entity = $obj1, tipType = 2, textKey = "gui_cashbox_only_number", time = 40)
        Return()
    }
    Else
    {

        If($money <= 0 or $money > 100)
        {
            ShowTip(entity = $obj1, tipType = 2, textKey = "gui_cashbox_only_number", time = 40)
            Return()
        }

        $owner = GetObject(objID = $ownerId)
        If(not $owner or not IsPlayer(entity = $owner))
        {
            Return()
        }

        If(not PayCurrency(entity = $obj1, coinName = "green_currency", count = $money))
        {
            ShowTip(entity = $obj1, tipType = 2, textKey = "gui_cashbox_not_enough_number", time = 40)
            Return()
        }

        AddCurrency(entity = $owner, coinName = "green_currency", count = $money)
        ShowRewardRollTip(image = "set:jail_break.json image:jail_break_currency", text = Concat(p1 = "+", p2 = $money), entity = $owner)
        ShowTip(entity = $owner, tipType = 2, textKey = "gui_cashbox_get_money", textP1 = GetEntityName(entity = $obj1), textP2 = $money, time = 40)
    }
}

