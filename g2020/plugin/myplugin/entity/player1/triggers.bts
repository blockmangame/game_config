            }
        }
    }
    $sellHouseMoney = 0
    $sellHouseCfg = DictionaryGet(dict = $curSelectDict, key = "houseCfg")
    $sellHouseMoney = $sellHouseMoney + GetEntityCfg(fullName = $sellHouseCfg, key = "sellMoney")
