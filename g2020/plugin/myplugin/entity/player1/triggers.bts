SELL_HOUSE{
    Test("SELL_HOUSE",$houseKey)
    $curSelectHouse = nil
    $houseVars = GetObjectVar(obj = $obj1, key = "houseVars")
    Foreach(array = $houseVars, key = "houseVar"){
        If(ArrayGet(array = $houseVar, index = 8) == $houseKey){
