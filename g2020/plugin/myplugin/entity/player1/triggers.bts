FUNC_GPS_SET_OR_SYNC --params: gpsDict
{
    If(not $gpsDict || DictionarySize(dict = $gpsDict) == 0)
    {
        Return()
    }
    
    $type = DictionaryGet(dict = $gpsDict, key = "type")
    If(not $type)
    {
        Return()
    }
    
    $belongMap = DictionaryGet(dict = $gpsDict, key = "belongMap")
    $pos = DictionaryGet(dict = $gpsDict, key = "pos")
    
    $conA = (($type == "map" || $type == "selfHouse" || $type == "region") && not $belongMap)
    $conB = $type == "entity" && (not $belongMap || not $pos)
    If($conA || $conB)
    {
        Return()
    }
    
    $curMapEntity = GetEntityMap(entity = $obj1)
    $curMap = GetEntityName(entity = $curMapEntity)
    $gps = { x = 0, y = 0, z = 0 }
    
    $owner = GetMapVar(map = $curMapEntity, key = "owner")
    $house = GetObjectVar(obj = $obj1, key = "house")
    If($owner)
    {
        $door = GetObjectVar(obj = $owner, key = "insideDoor")
        Test("type = ", $type)
        If($type == "selfHouse" && $curMapEntity == GetConfigVar(obj = $house, key = "houseInsideMap"))
        {
            ShowTip(entity = $obj1, tipType = 3, textKey = "you_came")
            CallTrigger(object = $obj1, event = "FUNC_CLOSE_GPS")
            Return()
        }
        $gps = GetEntityPosition(entity = $door)
    }
    ElseIf($curMap == $belongMap)
    {
        If($type == "map")
        {
            ShowTip(entity = $obj1, tipType = 3, textKey = "you_came")
            CallTrigger(object = $obj1, event = "FUNC_CLOSE_GPS")
            Return()
        }
        ElseIf($type == "region")
        {
            $region = GetRegionByName(map = $map, name = DictionaryGet(dict = $gpsDict, key = "regionName"))
            If($region)
            {
                $min = GetRegionMin(region = $region)
                $max = GetRegionMax(region = $region)
                $tmpReg = {
                    min = $min,
                    max = $max + V3(1, 1, 1)
                }
                If(IfEntityInArea(entity = $obj1, region = $tmpReg))
                {
                    ShowTip(entity = $obj1, tipType = 3, textKey = "you_came")
                    CallTrigger(object = $obj1, event = "FUNC_CLOSE_GPS")
                    Return()
                }
                $center = GetRegionCenter(region = $region)
                $gps = V3(GetPosX(pos = $center), GetPosY(pos = $min), GetPosZ(pos = $center))
            }
        }
        ElseIf($type == "entity")
        {
            $gps = DictionaryGet(dict = $gpsDict, key = "pos")
        }
        ElseIf($type == "selfHouse")
        {
            $housePos = GetEntityPosition(entity = $house)
            $gps = ComputeV3(p1 = $housePos, p2 = GetConfigVar(obj = $house, key = "doorPos") or {x = 0,y = 2, z = 0}, op = "add")
        }
    }
    Else
    {
        $road = []
        $maps = []
        $roadIdx = 1

        $ngt = DictionaryGet(dict = $gpsDict, key = "navigateDict")
        If(not $ngt)
        {
            CallTrigger(
                object = $obj1,
                event = "FUNC_GPS_RECALCULATE",
                mapStart = $curMap,
                mapEnd = $belongMap,
                gpsDict = $gpsDict,
            )
            Return()
        }
        Else
        {
            $roadIdx = DictionaryGet(dict = $ngt, key = "roadIdx") + 1
            $maps = DictionaryGet(dict = $ngt, key = "maps")
            $road = DictionaryGet(dict = $ngt, key = "road")
        
            $correctMap = ArrayGet(
                array = @allMaps,
                index = ArrayGet(
                    array = $maps,
                    index = $roadIdx
                )
            )
            If($correctMap ~= $mapName)
            {
                CallTrigger(
                    object = $obj1,
                    event = "FUNC_GPS_RECALCULATE",
                    mapStart = $curMap,
                    mapEnd = $belongMap,
                    gpsDict = $gpsDict,
                )
                Return()
            }
        
            $gps = GetConfigValue(
                type = "entity",
                key = "pos",
                fullName = ArrayGet(array = $road, index = $roadIdx)
            )
            
            $navigateDict = NewDictionary({
                road = $road,
                roadIdx = $roadIdx,
                maps = $maps
            })

            DictionarySet(
                dict = $gpsDict,
                key = "navigateDict",
                value = $navigateDict
            )
        }
    }

    CallTrigger(
        object = $obj1,
        event = "FUNC_OPEN_GPS",
        gps = $gps,
        gpsDict = $gpsDict
    )
}

FUNC_GPS_RECALCULATE --params: mapStart, mapEnd, gpsDict
{
    $road = []
    $maps = []
    $roadIdx = 1

    $mapAToBKey = Concat(p1 = $mapStart, p2 = "To", p3 = $mapEnd)
    $dwKey = Concat(p1 = $mapAToBKey, p2 = "DWs") --doorwayKey
    $maps = GetGlobalVar(key = $mapAToBKey) --maps
    $road = GetGlobalVar(key = $dwKey) --doorways
    If(not $road || not $maps)
    {
        ShowTip(entity = $obj1, tipType = 3, textKey = "unable_to_navigate", textP1 = $mapEnd)
        Return()
    }
    $mapSize = ArraySize(array = $maps)
    $matrix = GetGlobalVar(key = "mapMatrixNeedDijkstra")
    ForLoop(from = 1, to = $mapSize, key = "index")
    {
        $row = ArrayGet(array = $matrix, index = ArrayGet(array = $maps, index = $index))
        ForLoop(from = $index + 1, to = $mapSize, key = "idx")
        {
            If(ArrayGet(array = $row, index = $idx)) --从地图A到地图B存在多个门，那么即存在最短路的问题
            {   
                $type = DictionaryGet(dict = $gpsDict, key = "type")
                $belongMap = DictionaryGet(dict = $gpsDict, key = "belongMap")
                $targetPos = nil

                If($type == "selfHouse")
                {
                    $house = GetObjectVar(obj = $obj1, key = "house")
                    $housePos = GetEntityPosition(entity = $house)
                    $targetPos = ComputeV3(p1 = $housePos, p2 = GetConfigVar(obj = $house, key = "doorPos") or {x = 0,y = 2, z = 0}, op = "add")
                }
                ElseIf($type == "entity")
                {
                    $targetPos = DictionaryGet(dict = $gpsDict, key = "pos")
                }

                $targetDict = NewDictionary({
                    pos = $targetPos,
                    belongMap = $belongMap
                })

                --构建玩家在mapStart到mapEnd途径所有门的邻接矩阵
                $mapS2mapEDWsMatrix = "mapS2mapEDWsMatrix"
                CallTrigger(
                    object = $obj1,
                    allDWs = $road,
                    targetDict = $targetDict,
                    saveName = $mapS2mapEDWsMatrix,
                    event = "FUNC_NAVIGATION_SET_DOORWAY_ADJACENCY_MATRIX"
                )

                --利用上面的邻接矩阵进行最短路的查找操作
                $mapS2mapEDWs1Tother = Concat(p1 = $mapS2mapEDWsMatrix, p2 = "1Tother")
                CallTrigger(
                    object = $obj1,
                    startIdx = 1,
                    matrix = GetObjectVar(obj = $obj1, key = $mapS2mapEDWsMatrix),
                    saveName = $mapS2mapEDWs1Tother,
                    event = "FUNC_NAVIGATION_DIJKSTRA"
                )

                --利用上面的得到数据，再去找倒数第二张地图到最后一张地图需要经过的门，在这些门里找到dis最小的索引
                $index = 2
                If(not $targetPos)
                {
                    $secLastMap = ArrayGet(
                        array = @allMaps,
                        index = ArrayGet(array = $maps, index = $mapSize - 1)
                    )
                    $lastMap = ArrayGet(
                        array = @allMaps,
                        index = ArrayGet(array = $maps, index = $mapSize)
                    )
                    $mapEntry = NewDictionary(GetMapConfig(name = $secLastMap, key = "mapEntry"))
                    $entries = DictionaryGet(dict = $mapEntry, key = $lastMap)
                    $min = @mathHuge
                    $disArr = GetObjectVar(obj = $obj1, key = Concat(p1 = $mapS2mapEDWs1Tother, p2 = "Dis"))
                    ForLoop(from = 1, to = ArraySize(array = $entries or []), key = "idx")
                    {
                        $entryIdx = ArrayFind(
                            value = ArrayGet(array = $entries, index = $idx),
                            array = GetObjectVar(obj = $obj1, key = Concat(p1 = $mapS2mapEDWsMatrix, p2 = "ColVec"))
                        )
                        $curDis = ArrayGet(array = $disArr, index = $entryIdx)
                        If($curDis < $min)
                        {
                            $min = $curDis
                            $index = $entryIdx
                        }
                    }
                }

                --经过上面的操作后得到了到达最终地图的最短路径的最后一个索引
                $mapS2mapEDWsFinalPath = "mapS2mapEDWsFinalPath"
                CallTrigger(
                    object = $obj1,
                    endIdx = $index,
                    saveName = $mapS2mapEDWsFinalPath,
                    event = "FUNC_NAVIGATION_GET_PATH",
                    allPath = GetObjectVar(obj = $obj1, key = $mapS2mapEDWs1Tother)
                )
                $road = GetObjectVar(obj = $obj1, key = $mapS2mapEDWsFinalPath)
                $roadIdx = $roadIdx + 1 ---因为新的road第一个数据是obj1，如果是仅有唯一路径，则第一个数据就是第一个应该去的地方。

                SetObjectVar(obj = $obj1, value = nil, key = $mapS2mapEDWsFinalPath)
                SetObjectVar(obj = $obj1, value = nil, key = Concat(p1 = $mapS2mapEDWsMatrix, p2 = "ColVec"))
                SetObjectVar(obj = $obj1, value = nil, key = Concat(p1 = $mapS2mapEDWs1Tother, p2 = "Dis"))
                SetObjectVar(obj = $obj1, value = nil, key = $mapS2mapEDWs1Tother)
                SetObjectVar(obj = $obj1, value = nil, key = $mapS2mapEDWsMatrix)
                Break()
            }
        }
    }

    $navigateDict = NewDictionary({
        road = $road,
        roadIdx = $roadIdx,
        maps = $maps
    })

    DictionarySet(
        dict = $gpsDict,
        key = "navigateDict",
        value = $navigateDict
    )

    CallTrigger(
        object = $obj1,
        event = "FUNC_OPEN_GPS",
        gps = GetConfigValue(
            type = "entity",
            key = "pos",
            fullName = ArrayGet(array = $road, index = $roadIdx)
        ),
        gpsDict = $gpsDict
    )
}

