INVITE_JOIN_FAMILY
{
    $myFamilyID = GetEntityTeam(entity = $obj2)
    If(not $myFamilyID or $myFamilyID == 0)
    {
        Return()
    }

    $playerCount = GetTeamInfo(teamId = $myFamilyID, key = "playerCount")
    If(not $playerCount or $playerCount >= 4)
    {
        Return()
    }

    $obj1.inviteFamilyPlayer = $obj2

