SELL_HOUSE{
    Test("SELL_HOUSE",$houseKey)
    $curSelectDict = nil
    Foreach(array = GetObjectVar(obj = $obj1, key = "houseVars") or {}, key = "houseVar"){
        $dict = NewDictionary($houseVar)
        If(DictionaryGet(dict = $dict, key = "houseId") == $houseKey){
