        object = $obj1,
        event = "FUNC_STOP_TIMER",
        timerStr = Concat(p1 = $stateBase, p2 = "_TIMER")
    )

    $sTime = GetObjectVar(obj = $obj1, key = Concat(p1 = $stateBase, p2 = "STime"))
    $usedTime = GetObjectVar(obj = $obj1, key = Concat(p1 = $stateBase, p2 = "UsedTime"))
