REGION_ENTER {
    If(IsPlayer(entity = $obj1))
    {
        CallTrigger(
            event = "FUNC_REGION_STATE_OPERATION", 
            object = $obj1, 
            opt = "ENTER", 
            state = $inRegionKey,
        )
    }
}

REGION_LEAVE {
    If(IsPlayer(entity = $obj1))
    {
        CallTrigger(
            event = "FUNC_REGION_STATE_OPERATION",
            object = $obj1,
            opt = "LEAVE",
            state = $inRegionKey,
        )
    }
}